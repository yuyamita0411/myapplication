{
  "containerDefinitions": [
    {
      "name": "webappservice",
      "image": "011066745250.dkr.ecr.ap-northeast-1.amazonaws.com/app:myapp-php-fpm",
      "cpu": 150,
      "memoryReservation": 250,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "gwcloudwatch",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "php-fpm"
        }
      }
    },
    {
      "name": "webapp-nginx",
      "image": "011066745250.dkr.ecr.ap-northeast-1.amazonaws.com/app:myapp-nginx",
      "cpu": 200,
      "memoryReservation": 128,
      "links": [
        "webappservice"
      ],
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "dependsOn": [
        {
          "containerName": "webappservice",
          "condition": "START"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "gwcloudwatch",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "nginx"
        }
      }
    }
  ],
  "family": "webapp-nginx",
  "taskRoleArn": "arn:aws:iam::011066745250:role/ecs-task-role",
  "networkMode": "bridge",
  "volumes": [],
  "placementConstraints": []
}
